{
  "source": "./src",
  "destination": "./docs",
  "excludes": ["entry.js", ".spec.js"],
  "plugins": [{
    "name":"esdoc-standard-plugin",
    "option": {
      "test": {
        "source": "./src/",
        "interfaces": ["describe", "it", "context"],
        "includes": ["spec.js$"]
      },
			"brand": {
				"repository": "https://github.com/basic.webpack"
			}
    }
  }, {
    "name": "esdoc-flow-plugin"
  }, {
      "name": "esdoc-publish-html-plugin",
      "option": {
        "template": "docs/template"
      }
  }]
}
