    "content": "{\n  \"name\": \"LocalStorageDBApp\",\n  \"description\": \"Quick App for testing webpack building\",\n  \"author\": \"devlin.junker@gmail.com\",\n  \"license\": \"ISC\",\n  \"version\": \"0.0.1\",\n  \"main\": \" // TODO\",\n  \"scripts\": {\n    \"dev-watch\": \"./node_modules/.bin/webpack --watch\",\n    \"dev\": \"./node_modules/.bin/webpack-dev-server --open\",\n    \"test\": \"node_modules/karma/bin/karma start --single-run\",\n    \"test-watch\": \"node_modules/karma/bin/karma start\",\n    \"start-watch\": \"(sleep 7s && open http://localhost:3000) & node_modules/karma/bin/karma start\",\n    \"doc\": \"./node_modules/.bin/esdoc\",\n    \"build\": \"./node_modules/.bin/webpack\",\n    \"clean\": \" ## TODO: clean up project files\"\n  },\n  \"dependencies\": {\n    \"lodash\": \"^4.17.11\"\n  },\n  \"devDependencies\": {\n    \"@babel/core\": \"^7.3.3\",\n    \"@babel/polyfill\": \"^7.2.5\",\n    \"@babel/preset-env\": \"^7.3.1\",\n    \"@babel/preset-flow\": \"^7.0.0\",\n    \"@babel/register\": \"^7.0.0\",\n    \"babel-eslint\": \"^10.0.1\",\n    \"babel-loader\": \"^8.0.5\",\n    \"chai\": \"^4.2.0\",\n    \"chai-as-promised\": \"^7.1.1\",\n    \"clean-webpack-plugin\": \"^1.0.1\",\n    \"css-loader\": \"^2.1.0\",\n    \"esdoc\": \"^1.1.0\",\n    \"esdoc-flow-plugin\": \"^1.0.0\",\n    \"esdoc-standard-plugin\": \"^1.0.0\",\n    \"eslint\": \"^5.15.1\",\n    \"eslint-config-prettier\": \"^4.1.0\",\n    \"eslint-import-resolver-webpack\": \"^0.11.0\",\n    \"eslint-loader\": \"^2.1.2\",\n    \"eslint-plugin-flowtype\": \"^3.4.2\",\n    \"eslint-plugin-header\": \"^3.0.0\",\n    \"eslint-plugin-import\": \"^2.16.0\",\n    \"eslint-plugin-prettier\": \"^3.0.1\",\n    \"file-loader\": \"^3.0.1\",\n    \"flow-bin\": \"^0.93.0\",\n    \"flow-typed\": \"^2.5.1\",\n    \"flow-webpack-plugin\": \"^1.2.0\",\n    \"html-webpack-plugin\": \"^3.2.0\",\n    \"ignore-styles\": \"^5.0.1\",\n    \"karma\": \"^3.1.4\",\n    \"karma-chrome-launcher\": \"^2.2